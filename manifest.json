{
  "manifest_version": 3,
  "name": "Pikkoro Video Streaming",
  "short_name": "Piccolo",
  "start_url": "/Namekind.github.io/index.html",
  "display": "standalone",
  "theme_color": " #8A2BE2",
  "background_color": "black",
  "orientation": "any",
  "description": "We just cured boredom and depression with a PWA...",
  "permissions": [],
  "serviceworker": {
    "src": "/sw.js"
  }
  "icons": [
    {
      "src": "appicon.png",
      "sizes": "72x72",
      "type": "image/png"
    },
    {
      "src": "appicon.png",
      "sizes": "96x96",
      "type": "image/png"
    },
    {
      "src": "appicon.png",
      "sizes": "144x144",
      "type": "image/png"
    },
    {
      "src": "appicon.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "appicon.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "categories": [
    "entertainment",
    "media"
  ],
  "splash_pages": null,
  "related_applications": [
    {
      "platform": "play",
      "url": "https://play.google.com/store/apps/details?id=com.piccolo.streaming",
      "id": "com.piccolo.streaming"
    }
  ],
  "prefer_related_applications": true
}
